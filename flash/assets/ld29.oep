<project>
	<name>LD29: Beneath the Surface</name>
	<settings>  
		<defaultWidth>210</defaultWidth>
		<defaultHeight>90</defaultHeight>
		<minWidth>210</minWidth>
		<minHeight>90</minHeight>
		<maxWidth>5000</maxWidth>
		<maxHeight>5000</maxHeight>
	</settings>
	<values>
		<string name="title" default="LD29 level" maxChars="30" />
	</values> 
	<layers>  
		<tiles name="parallax" gridSize="24"/>
		<tiles name="background" gridSize="24"/>
		<tiles name="terrain" gridSize="24"/>
		<tiles name="foreground" gridSize="24"/>
		<objects name="objects" gridSize="24"/>
	</layers>
	<tilesets> 
		<tileset name="tiles" image="tiles.png" tileWidth="24" tileHeight="24"/>
	</tilesets>   
	<objects>
		<object name="enemy" image="baddie_placeholder.png" width="16" height="16"/>
		<object name="whale" image="whale_placeholder.png" width="96" height="32"/>
		<object name="shark" image="shark_placeholder.png" width="64" height="24"/>
		<object name="angler_fish" image="angler_fish.png" width="16" height="16"/>
		<object name="shipwreck" image="shipwreck.png" width="72" height="24"/>
		<object name="boat_wreck" image="shipwreck2.png" width="72" height="24"/>
		<object name="capital_sub" image="capital_sub_placeholder.png" width="72" height="24"/>
		<object name="treasure" image="treasure_placeholder.png" width="16" height="16">
		    <values>
		        <integer name="value"/>
		    </values>
		</object>
		<object name="crate" image="crate.png" width="16" height="16">
		    <values>
		        <integer name="value"/>
		    </values>
		</object>
	</objects>
</project>